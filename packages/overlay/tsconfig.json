{
    "extends": "../../configs/tsconfig.base.json",
    "compilerOptions": {
        "jsx": "react",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "baseUrl": "./src",
        "paths": {
            "@hooks/*": ["hooks/*"],
            "@utils/*": ["utils/*"],
            "@components/*": ["components/*"],
            "@models/*": ["models/*"],
            "@type/*": ["type/*"],
            "@css/*": ["css/*"],
            "@icons/*": ["icons/*"],
            "@atoms/*": ["atoms/*"]
        },
        "types": ["chrome"]
    },
    "include": ["src"]
}
