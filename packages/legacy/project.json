{
    "root": "packages/legacy",
    "sourceRoot": "packages/legacy/src",
    "projectType": "application",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "./packages/legacy/config/jest.config.ts"
            },
            "outputs": [],
            "dependsOn": [
                {
                    "target": "test",
                    "projects": "dependencies"
                }
            ]
        },
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": ["build"],
            "options": {
                "command": "./node_modules/webpack-cli/bin/cli.js --config ./webpack.prod.js",
                "cwd": "packages/legacy"
            },
            "dependsOn": [
                {
                    "target": "test",
                    "projects": "self"
                }
            ]
        }
    }
}
